client
dev tun
proto tcp
resolv-retry infinite
nobind
persist-key
persist-tun
comp-lzo adaptive
verb 3
remote {{ domain }} {{ port }}
route-delay
reneg-sec 0
tls-auth ta.key 1
tls-version-min 1.2
cipher AES-256-CBC
auth SHA512
tls-cipher TLS-DHE-RSA-WITH-AES-256-GCM-SHA384:TLS-DHE-RSA-WITH-AES-128-GCM-SHA256:TLS-DHE-RSA-WITH-AES-256-CBC-SHA:TLS-DHE-RSA-WITH-CAMELLIA-256-CBC-SHA:TLS-DHE-RSA-WITH-AES-128-CBC-SHA:TLS-DHE-RSA-WITH-CAMELLIA-128-CBC-SHA
redirect-gateway
script-security 2
--auth-user-pass
<ca>
{{ ca_yunohost }}
</ca>
user openvpn
group openvpn
key-direction 1
<tls-auth>
{{ ta_key }}
</tls-auth>
